            android:layout_width="wrap_content"
            android:layout_x="@dimen/p02_butterfly_x"
            android:layout_y="@dimen/p02_butterfly_y"
            />

    <andr.lexibook.mylittlestory.tlps.ui.ViewIml.GifMovieView
            android:id="@+id/gif_p02_wolf"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_x="@dimen/p02_wolf_x"
            android:layout_y="@dimen/p02_wolf_y"
            />

</AbsoluteLayout>