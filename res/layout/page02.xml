                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/layout_p02"
        >

    <andr.lexibook.mylittlestory.tlps.ui.ViewIml.GifMovieView
            android:id="@+id/gif_p02_flower"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_x="@dimen/p02_flower_x"
            android:layout_y="@dimen/p02_flower_y"
            />

    <andr.lexibook.mylittlestory.tlps.ui.ViewIml.GifMovieView
            android:id="@+id/gif_p02_butterfly"
