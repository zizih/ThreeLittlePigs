<?xml version="1.0" encoding="utf-8"?>

<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:id="@+id/layout_p07"
        >
    <LinearLayout
            android:layout_height="fill_parent"
            android:layout_width="fill_parent"
            android:background="@drawable/p07_bkg_cover"></LinearLayout>

    <andr.lexibook.mylittlestory.tlps.ui.ViewIml.GifMovieView
            android:id="@+id/gif_p07_pig_brown"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_x="@dimen/p07_pig_brown_x"
            android:layout_y="@dimen/p07_pig_brown_y"
            />
    <andr.lexibook.mylittlestory.tlps.ui.ViewIml.GifMovieView
            android:id="@+id/gif_p07_pig_yel"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_x="@dimen/p07_pig_yel_x"
            android:layout_y="@dimen/p07_pig_yel_y"
            />
    <andr.lexibook.mylittlestory.tlps.ui.ViewIml.GifMovieView
            android:id="@+id/gif_p07_star"
            android:layout_height="fill_parent"
            android:layout_width="fill_parent"
            android:layout_x="@dimen/p07_star_x"
            android:layout_y="@dimen/p07_star_y"
            />

</AbsoluteLayout>